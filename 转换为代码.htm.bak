<script>
	function $(s){
		return document.getElementById(s);
	} 
	function xx(){
		var flag=$("sl").value;
		if(flag=="Video"){
			//Videos[Videos.length++]=new Array("","","","","","","");
			//title[0],CH[1],description[2],tags[3](connect by "&&&"),url[4],color[5],type[6](mv,dongman,movie...)
			var txt="Videos[Videos.length++]=new Video(\""+ $("Title").value +"\",\""+ $("Description").value +"\",\""+ $("Tags").value +"\",\""+ $("Src").value +"\",\""+ $("Color").value +"\",\""+ $("Ground").value +"\",\""+ $("CH").value +"\");";
			clipboardData.setData('Text',txt);
			$("temp").value=txt;
		}
		if(flag=="Song"){
			var txt="Songs[Songs.length++]=new Array(\""+ $("Title").value.replace(/\'/g,"\\\'") +"\",\""+ $("CH").value +"\",\""+ $("singer").value +"\",\""+ $("singerCH").value +"\",\""+ $("Src").value +"\");";
			clipboardData.setData('Text',txt);
			clipboardData.setData('Text',txt);
			$("temp").value=txt;
		}
		if(flag=="Lrc"){
			var txt="Lrcs[Lrcs.length++]=new Array('"+ $("Title").value +"','"+ 
				$("txt").value.replace(/\[\d\d:\d\d\.\d\d\]/g,"").replace(/\r\n/g,"<br>").replace(/\'/g,"\\\'").replace(/\"/g,"\\\"")
				.replace(/\//g,"\\\/").replace(/\$/g,"\\\$").replace(/\(/g,"\\\(").replace(/\)/g,"\\\)").replace(/\*/g,"\\\*")
				.replace(/\+/g,"\\\+").replace(/\./g,"\\\.").replace(/\?/g,"\\\?").replace(/\^/g,"\\\^").replace(/\{/g,"\\\{")
				.replace(/\|/g,"\\\|").replace(/\}/g,"\\\}")
			 +"');";
			clipboardData.setData('Text',txt);
			$("temp").value=txt;
		}
		alert("复制成功");
		$("txt").value="";
		$("txt").focus();
		
	}
</script>
